{
  "name": "vue-dragndrop",
  "private": true,
  "description": "Drag and Drop Components For VueJS",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-lib-draggable": "vue-cli-service build --target lib --name vue-draggable --no-clean ./packages/draggable/src/index.ts",
    "build-lib-dropzone": "vue-cli-service build --target lib --name vue-dropzone --no-clean ./packages/dropzone/src/index.ts",
    "build-lib-dragndrop": "vue-cli-service build --target lib --name vue-dragndrop --no-clean ./packages/dragndrop/src/index.ts",
    "build-lib": "npm run build-lib-draggable & npm run build-lib-dropzone",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^3.3.2",
    "vue": "^2.6.10",
    "vue-class-component": "^7.0.2",
    "vue-property-decorator": "^8.3.0"
  },
  "devDependencies": {
    "@dragndrop/draggable": "^0.1.4",
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-plugin-typescript": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "@vue/eslint-config-typescript": "^4.0.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "typescript": "~3.5.3",
    "vue-template-compiler": "^2.6.10"
  }
}
